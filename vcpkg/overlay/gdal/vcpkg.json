{
  "name": "gdal",
  "version-semver": "3.2.2",
  "description": "The Geographic Data Abstraction Library for reading and writing geospatial raster and vector data",
  "homepage": "https://gdal.org",
  "supports": "!arm",
  "dependencies": [
    "cfitsio",
    "curl",
    "expat",
    "geos",
    "giflib",
    "libgeotiff",
    "liblzma",
    "libpng",
    "libpq",
    "libwebp",
    "libxml2",
    "openjpeg",
    "proj4",
    "sqlite3",
    "zlib",
    "zstd"
  ],
  "features": {
    "libspatialite": {
      "description": "Create or update SpatiaLite databases using libspatialite",
      "dependencies": [
        "libspatialite"
      ]
    },
    "mysql-libmariadb": {
      "description": "Add mysql support using libmariadb",
      "dependencies": [
        "libmariadb"
      ]
    },
    "hdf5": {
      "description": "Support for HDF5",
      "dependencies": [
        "hdf5",
        {
          "name": "json-c",
          "platform": "!windows"
        }
      ]
    },
    "netcdf": {
      "description": "Support for netcdf",
      "dependencies": [
        "netcdf-c"
      ]
    }
  }
}
